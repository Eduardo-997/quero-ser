#include <stdio.h>
#include <stdlib.h>


int main()
{
	
	int numero[100], cont, cont2;
	
	for (cont=1;cont<=100;cont++)
	{
		numero[cont]=cont;
	}
	
	for (cont2=1;cont2<=100;cont2++)
	{
		
		 if ((cont2%3==0 && cont2%5==0))
		{
			printf("\n FizzBuzz \n");
		}	
	
		else if(cont2%3==0)
		{
			printf("\n Fizz \n");
		}
		
		else if(cont2%5==0)
		{
			printf("\n Buzz \n");
		}
		
		else if  ((cont2%3!=0) && (cont2%5!=0))
		{
			printf ("\n %d \n", numero[cont2]);
		}
	}
	
	
	return 0;
}
